*/\n  { path: \"system\",\n    children:[{ path: \"system/user\", component: User },\n      { path: \"system/data\", component: ComplexForm },\n      { path: \"system/config\", component: ComplexForm }]\n  },\n];\n\nexport default routeConfigs;\n\ninterface RouterConfigItem {\n  path: string;\n  component?: FC | ComponentClass;\n  children?:Array<RouterConfigItem>;\n}\n","/home/iluvsmile/IdeaProjects/qinyuanjie/ys-web-boilerplate/src/pages/404.tsx",[],"/home/iluvsmile/IdeaProjects/qinyuanjie/ys-web-boilerplate/src/components/Menu/index.tsx",["203","204"],"/** 左侧菜单 */\nimport { FC, useMemo } from \"react\";\nimport { Link, RouteComponentProps, withRouter } from \"react-router-dom\";\nimport styled from \"styled-components\";\nimport routeConfigs from \"../../configs/routeConfigs\";\nimport { Menu as AntdMenu } from \"antd\";\nimport { useTranslation } from \"react-i18next\";\n\nconst StyledMenu = styled.div`\n  width: 15rem;\n  height: 100%;\n  border-right: 1px solid #f0f0f0;\n  margin: 2rem 0;\n`;\n\nconst Menu: FC<RouteComponentProps> = ({ location }) => {\n  const { t } = useTranslation(\"menu\");\n\n  const activity = useMemo(() => {\n      let act = routeConfigs.find((r) => {\n          if(!r.children){\n              return  location.pathname.indexOf(r.path) === 1\n          }\n      })?.path || \"\"\n      if(!act){\n          routeConfigs.map((c)=>{\n              if(c.children && !act){\n                  act = c.children.find((r) => location.pathname.indexOf(r.path) === 1)?.path || \"\"\n              }\n          })\n      }\n    return (act);\n  }, [location]);\n\n  const menus = useMemo(() => {\n    return routeConfigs.map((c) => {\n      let menu ;\n      if(!c.children){\n      menu=\n        <AntdMenu.Item style={{ fontSize: \"1rem\" }} key={c.path}>\n          <Link to={`/${c.path}`}>{t(c.path)}</Link>\n        </AntdMenu.Item>\n      }else{\n        let children =\n            c.children.map((gc) => {\n              return (<AntdMenu.Item style={{ fontSize: \"1rem\" }} key={gc.path}>\n                <Link to={`/${gc.path}`}>{t(gc.path)}</Link>\n              </AntdMenu.Item>)\n            })\n        menu = <AntdMenu.ItemGroup title={t(c.path)} key={c.path}>\n          {children}\n        </AntdMenu.ItemGroup>\n      }\n      return (menu);\n    });\n  }, [t]);\n\n  return (\n    <StyledMenu>\n      <AntdMenu mode=\"inline\" selectedKeys={[activity]}>\n        {menus}\n      </AntdMenu>\n    </StyledMenu>\n  );\n};\n\nexport default withRouter(Menu);\n","/home/iluvsmile/IdeaProjects/qinyuanjie/ys-web-boilerplate/src/components/Header/index.tsx",[],"/home/iluvsmile/IdeaProjects/qinyuanjie/ys-web-boilerplate/src/pages/LoadingPage/index.tsx",[],"/home/iluvsmile/IdeaProjects/qinyuanjie/ys-web-boilerplate/src/components/Footer/index.tsx",[],"/home/iluvsmile/IdeaProjects/qinyuanjie/ys-web-boilerplate/src/pages/ErrorPage/index.tsx",[],"/home/iluvsmile/IdeaProjects/qinyuanjie/ys-web-boilerplate/src/components/DemoForm/BasicForm.tsx",[],"/home/iluvsmile/IdeaProjects/qinyuanjie/ys-web-boilerplate/src/components/DemoForm/StepForm.tsx",[],"/home/iluvsmile/IdeaProjects/qinyuanjie/ys-web-boilerplate/src/components/DemoForm/ComplexForm.tsx",[],"/home/iluvsmile/IdeaProjects/qinyuanjie/ys-web-boilerplate/src/components/Demo/index.ts",[],"/home/iluvsmile/IdeaProjects/qinyuanjie/ys-web-boilerplate/src/components/StyledComponents/index.ts",[],"/home/iluvsmile/IdeaProjects/qinyuanjie/ys-web-boilerplate/src/components/Demo/Demo.tsx",[],"/home/iluvsmile/IdeaProjects/qinyuanjie/ys-web-boilerplate/src/components/YSDatePicker/index.tsx",[],"/home/iluvsmile/IdeaProjects/qinyuanjie/ys-web-boilerplate/src/components/Demo/demo.services.ts",[],"/home/iluvsmile/IdeaProjects/qinyuanjie/ys-web-boilerplate/src/components/StyledComponents/StyledContainer.tsx",[],"/home/iluvsmile/IdeaProjects/qinyuanjie/ys-web-boilerplate/src/components/Demo/DemoForm.tsx",[],"/home/iluvsmile/IdeaProjects/qinyuanjie/ys-web-boilerplate/src/components/Demo/DemoTab