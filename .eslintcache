interceptors.request.use(requestOnFullFilled);\nYSAxios.interceptors.response.use(onFullFilled, onRejected);\n\nconst clearAndBackLogin = () => {\n  clearToken();\n  setPreLocation();\n  history.push(\"/login\");\n};\n\nexport default YSAxios;\n\nexport const isOk = (response: AxiosResponse) =>\n  response && response.status >= 200 && response.status < 300;\n\nexport const callback = (\n  response: AxiosResponse,\n  cb: AsyncCallback,\n  defaultValue?: any\n) => cb({ isOk: isOk(response), data: response?.data || defaultValue });\n","/home/iluvsmile/IdeaProjects/qinyuanjie/ys-web-boilerplate/src/configs/api.ts",[],"/home/iluvsmile/IdeaProjects/qinyuanjie/ys-web-boilerplate/src/constants/strings.ts",[],"/home/iluvsmile/IdeaProjects/qinyuanjie/ys-web-boilerplate/src/models/dict.ts",[],"/home/iluvsmile/IdeaProjects/qinyuanjie/ys-web-boilerplate/src/utils/tokenUtils.ts",[],"/home/iluvsmile/IdeaProjects/qinyuanjie/ys-web-boilerplate/src/utils/preLocationUtils.ts",[],"/home/iluvsmile/IdeaProjects/qinyuanjie/ys-web-boilerplate/src/utils/apiUtils.ts",[],"/home/iluvsmile/IdeaProjects/qinyuanjie/ys-web-boilerplate/src/utils/jwtDecode.js",[],["206","207"],"/home/iluvsmile/IdeaProjects/qinyuanjie/ys-web-boilerplate/src/utils/localStorageHelper.ts",[],"/home/iluvsmile/IdeaProjects/qinyuanjie/ys-web-boilerplate/src/components/YSSearchBar/index.tsx",[],"/home/iluvsmile/IdeaProjects/qinyuanjie/ys-web-boilerplate/src/components/YSTable/index.tsx",[],"/home/iluvsmile/IdeaProjects/qinyuanjie/ys-web-boilerplate/src/pages/System/User.tsx",[],"/home/iluvsmile/IdeaProjects/qinyuanjie/ys-web-boilerplate/src/pages/System/index.ts",[],"/home/iluvsmile/IdeaProjects/qinyuanjie/ys-web-boilerplate/src/pages/System/user.services.ts",[],"/home/iluvsmile/IdeaProjects/qinyuanjie/ys-web-boilerplate/src/pages/System/UserSearch.tsx",[],"/home/iluvsmile/IdeaProjects/qinyuanjie/ys-web-boilerplate/src/pages/System/UserTable.tsx",["208","209","210"],"import dayjs from \"dayjs\";\nimport { FC, useMemo } from